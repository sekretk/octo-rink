<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
                    xmlns:options="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options">
    
    <Color x:Key="TransparentColor" options:Freeze="True">#00FFFFFF</Color>
    <Color x:Key="SemitransparentColor" options:Freeze="True">#20FFFFFF</Color>
    <Color x:Key="BackgroundColor" options:Freeze="True">#FFFFFFFF</Color>
    <Color x:Key="ForegroundColor" options:Freeze="True">#FF000000</Color>
    <Color x:Key="HighlightColor" options:Freeze="True">#FFDEDEDE</Color>
    <Color x:Key="MiddleLightColor" options:Freeze="True">#FF777777</Color>
    <Color x:Key="LowlightColor" options:Freeze="True">#FF4D4D4D</Color>
    <Color x:Key="DisabledColor" options:Freeze="True">#FFB8B5B2</Color>
    
</ResourceDictionary>